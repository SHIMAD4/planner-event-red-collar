{
  "extends": [
    "@rc-foundation/eslint-config-rc",
    "@rc-foundation/eslint-config-rc/unicorn",
    "@rc-foundation/eslint-config-rc/fsd-import"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
}
